{% extends 'base.twig' %}
{% block content %}

    <div class="container text-center mt-4">
        <div class="row justify-content-md-center">
            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Номер дела</th>
                    <th scope="col">Лот</th>
                    <th scope="col">Площадка</th>
                    <th scope="col">Тип</th>
                    <th scope="col">Email контактного лица</th>
                    <th scope="col">Номер контактного лица</th>
                    <th scope="col">ИНН должника</th>
                    <th scope="col">Дата проведения</th>
                </tr>
                </thead>
                <tbody class="tableBody">
                {% for d in data %}
                    <tr>
                        <th scope="row">
                            <a href="https://fedresurs.ru/bidding/{{ d.guid }}" target="_blank">{{ d.num }}
                            </a></th>
                        <td>{{ d.lot }}</td>
                        <td>{{ d.place }}</td>
                        <td>{{ d.type }}</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>

                    </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{% endblock %}